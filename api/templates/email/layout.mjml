<mjml>
  <mj-body background-color="#E7EDF7">
    <mj-section></mj-section>
    <mj-wrapper background-color="#FFFFFF" border="solid 1px #E7EDF7" padding="0" border-radius="16px">
      <mj-section padding="24px 0">
        <mj-column width="128px">
          <mj-image width="128px" src="data:image/png;base64,">
          </mj-image>
        </mj-column>
        <mj-column width="470px">
          <mj-text font-size="32px" font-weight="900" padding="24px 24px 0" color="#06161A">{{ instance['name'] }}</mj-text>
        </mj-column>
      </mj-section>
      <mj-section padding-top="0">
        <mj-column>
          <mj-divider padding-top="0" border-width="1px" border-color="#C8D1F4"></mj-divider>
          <mj-text font-size="18px" color="#06161A">{% block content %}{% endblock %}</mj-text>
        </mj-column>
      </mj-section>
    </mj-wrapper>
    <mj-section padding="0">
    	<mj-column>
      	<mj-text padding="16px 0" font-size="14px" color="#0E4F5F">This email have been sent to you from the reel2bits instance '{{ instance['name'] }}' located at {{ instance['url'] }} 
        </mj-text>
      </mj-column>
    </mj-section>
  </mj-body>
</mjml>